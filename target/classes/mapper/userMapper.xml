<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="userDao">
	<insert id="insertUser" parameterType="user">
		INSERT INTO tb_user
		(OAUTH_ID, U_NAME)
		VALUES
		(#{oauth_id}, #{u_name})	
	</insert>
</mapper>
